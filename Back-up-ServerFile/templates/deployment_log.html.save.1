        <div class="content-body">
            <table>
                <thead>
                    <tr>
                        <th>Hostname</th>
                        <th>IP</th>
                        <th>Template Name</th>
                        <th>Status</th>
                        <th>Details</th>
                    </tr>
                </thead>
                <tbody>
                    {% for log in deployment_logs %}
                    <tr>
                        <td>{{ log.hostname }}</td>
                        <td>{{ log.ip }}</td>
                        <td>{{ log.template_name }}</td>
                        <td>
                            <span class="{{ 'status-success' if log.status == 'Success' else 'status-failure' }}">
                                {{ log.status }}
                            </span>
                        </td>
                        <td><pre>{{ log.details }}</pre></td>
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
